        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::marketing">
                    <resource id="Mymodule_SEO::helloworld" title="HelloWorld" sortOrder="60">
                        <resource id="Mymodule_SEO::index" title="HelloWorld index" />
